{
  "name": "archen",
  "version": "1.1.5",
  "description": "Archen - Add GraphQL API to your existing applications",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "author": "Weidong Fang",
  "license": "MIT",
  "dependencies": {
    "dataloader": "^1.4.0",
    "graphql": "^0.13.2",
    "sqlit": ">=1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/fangwd/archen.git"
  },
  "devDependencies": {
    "@types/graphql": "^0.12.5",
    "@types/jest": "^22.2.0",
    "@types/mysql": "^2.15.4",
    "@types/node": "^9.4.7",
    "typescript": "*",
    "jest": "^22.4.1",
    "ts-jest": "^22.4.1"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/test/.+\\.test.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "verbose": true
  }
}
